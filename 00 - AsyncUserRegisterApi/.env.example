# Environment Variables Example
# Copy this file to .env and update with your actual values

# AWS Configuration
AWS_REGION=us-east-1
AWS_PROFILE=default

# Deployment Stage
STAGE=dev

# API URLs (populated after deployment)
API_BASE_URL=https://your-api-id.execute-api.us-east-1.amazonaws.com/dev
WEBSOCKET_URL=wss://your-websocket-id.execute-api.us-east-1.amazonaws.com/dev

# DynamoDB Table Names (auto-generated by serverless)
USERS_TABLE=async-user-register-api-users-dev
BOARDS_TABLE=async-user-register-api-boards-dev
MESSAGES_TABLE=async-user-register-api-messages-dev
CONNECTIONS_TABLE=async-user-register-api-connections-dev

# SNS Topics (auto-generated by serverless)
USER_REGISTRATION_TOPIC=async-user-register-api-user-registration-dev
MESSAGE_POSTING_TOPIC=async-user-register-api-message-posting-dev

# SQS Queue (auto-generated by serverless)
BOARD_CREATION_QUEUE=async-user-register-api-board-creation-dev
